-- This is a temp table consolidating user labels over the dates intervals.
CREATE OR REPLACE TEMP TABLE training_preparation_label as (
  SELECT DISTINCT
  LABEL.user_pseudo_id, -- The unique identifier for the user.
  LABEL.feature_date, -- The date for which the features are extracted.
  MAX(LABEL.purchase_day_1) OVER(purchase_propensity_label_window) AS purchase_day_1, -- Whether the user made a purchase on day 1.
  MAX(LABEL.purchase_day_2) OVER(purchase_propensity_label_window) AS purchase_day_2, -- Whether the user made a purchase on day 2.
  MAX(LABEL.purchase_day_3) OVER(purchase_propensity_label_window) AS purchase_day_3, -- Whether the user made a purchase on day 3.
  MAX(LABEL.purchase_day_4) OVER(purchase_propensity_label_window) AS purchase_day_4, -- Whether the user made a purchase on day 4.
  MAX(LABEL.purchase_day_5) OVER(purchase_propensity_label_window) AS purchase_day_5, -- Whether the user made a purchase on day 5.
  MAX(LABEL.purchase_day_6) OVER(purchase_propensity_label_window) AS purchase_day_6, -- Whether the user made a purchase on day 6.
  MAX(LABEL.purchase_day_7) OVER(purchase_propensity_label_window) AS purchase_day_7, -- Whether the user made a purchase on day 7.
  MAX(LABEL.purchase_day_8) OVER(purchase_propensity_label_window) AS purchase_day_8, -- Whether the user made a purchase on day 8.
  MAX(LABEL.purchase_day_9) OVER(purchase_propensity_label_window) AS purchase_day_9, -- Whether the user made a purchase on day 9.
  MAX(LABEL.purchase_day_10) OVER(purchase_propensity_label_window) AS purchase_day_10, -- Whether the user made a purchase on day 10.
  MAX(LABEL.purchase_day_11) OVER(purchase_propensity_label_window) AS purchase_day_11, -- Whether the user made a purchase on day 11.
  MAX(LABEL.purchase_day_12) OVER(purchase_propensity_label_window) AS purchase_day_12, -- Whether the user made a purchase on day 12.
  MAX(LABEL.purchase_day_13) OVER(purchase_propensity_label_window) AS purchase_day_13, -- Whether the user made a purchase on day 13.
  MAX(LABEL.purchase_day_14) OVER(purchase_propensity_label_window) AS purchase_day_14, -- Whether the user made a purchase on day 14.
  MAX(LABEL.purchase_day_15_30) OVER(purchase_propensity_label_window) AS purchase_day_15_30 -- Whether the user made a purchase between days 15 and 30.
FROM
  `{{feature_store_project_id}}.{{feature_store_dataset}}.purchase_propensity_label` LABEL
WHERE
  -- Define the training subset interval
  LABEL.feature_date BETWEEN GREATEST(start_date, min_date) AND LEAST(end_date, max_date)
WINDOW 
  purchase_propensity_label_window AS (PARTITION BY LABEL.user_pseudo_id, LABEL.feature_date ROWS BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING)
);

