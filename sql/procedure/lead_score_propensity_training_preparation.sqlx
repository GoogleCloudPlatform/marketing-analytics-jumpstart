  SELECT DISTINCT
    -- User pseudo id
    UWLM.user_pseudo_id,
    -- Feature date
    UWLM.feature_date,
    -- Calculate the maximum value for each metric over the window
    MAX(UWLM.scroll_50_past_1_day) OVER(user_rolling_lead_window) AS scroll_50_past_1_day,
    -- Maximum active users in the past 2 days
    MAX(UWLM.scroll_50_past_2_day) OVER(user_rolling_lead_window) AS scroll_50_past_2_day,
    -- Maximum active users in the past 3 days
    MAX(UWLM.scroll_50_past_3_day) OVER(user_rolling_lead_window) AS scroll_50_past_3_day,
