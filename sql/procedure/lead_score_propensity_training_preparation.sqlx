    -- Maximum recipe_favorite in the past 4 days
    MAX(UWLM.recipe_favorite_past_4_day) OVER(user_rolling_lead_window) AS recipe_favorite_past_4_day,
    -- Maximum recipe_favorite in the past 5 days
    MAX(UWLM.recipe_favorite_past_5_day) OVER(user_rolling_lead_window) AS recipe_favorite_past_5_day,
    -- Maximum recipe_add_to_menu in the past 1 day
    MAX(UWLM.recipe_add_to_menu_past_1_day) OVER(user_rolling_lead_window) AS recipe_add_to_menu_past_1_day,
